<th style="border: 1px solid white; padding:4px 10px 4px 10px;">Action</th></tr>
<% for(var i in data) 
	            {                     
            %>
<tr style="background-color:#2c3e50; border: 1px solid white;">
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].firstname %> <br/>  
				
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].lastname %> <br/>  
         
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].email %> <br/>  
         
 </td>
 <td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].password %> <br/> 
          
 </td>
 <td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].mobile %> <br/> 
          
 </td>
  
  <td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].billingid %> <br/> 
         
 </td>
  
  <td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].type %> <br/> 
         
 </td>
 
 <td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].securityques %> <br/> 
         
 </td>
 
  <td style="border: 1px solid white;padding:4px 10px 4px 10px; color:black;">
<!-- <button type="button" value="delete" onclick="javascript:sensordelete('/sensordelete','<%= data[i].id_primarykey %>');">delete</button> -->
	<button type="button" value="delete" onclick="javascript:sensoredit('/sensoredit','<%= data[i].id_primarykey %>');">Edit</button><br/>
   </td>
</tr>
  <% 
	            }  
	       %>
