function sensordelete(action,id)
{
	$('form#form1').attr('action',action);
	$('#sensorid').val(id);
	$('form#form1').submit();
}

function sensoredit(action,id)
{
	$('form#form1').attr('action',action);
	$('#sensorid').val(id);
	$('form#form1').submit();
}
</script>
