<div  style="color:white;margin-left:128px;"><H2>Sensor Data Information<H2></div>
</br>
<div style="color:white;margin-left:58px;">
<form style="margin: 4px 4px 4px 4px;" method="get" action="/vendor_editcontroller" id="form1" >  
<input type="hidden" name="sensorid" id="sensorid" />
<table class="container" style="border:1px solid white;">
<tr style="background-color:#2c3e50; border: 1px solid white;">
<th style="border: 1px solid white; padding:4px 10px 4px 10px;">Sensor Id</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;"> Sensor name </th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Status</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Sensor location</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Sensor type</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Sensor Data</th>
</tr>  
<% for(var i in data) 
	            {                     
            %>
            <tr style="background-color:#2c3e50; border: 1px solid white;">
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].sensorid %> <br/>  
				
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					<%= data[i].sensorname %> <br/>  
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					<%= data[i].status %> <br/>  
 </td>
 <td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					 <%= data[i].sensorlocation %> <br/> 
 </td>
 <td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					 <%= data[i].sensortype %> <br/> 
 </td>
 <td><button type="button" style="width:80px;color:black;margin-left:10px;" onclick="go('/singleSensorData','<%= data[i].sensorid %>');" >data</button> </td>
 
 
</tr>
<% 
	            }  
	       %>
</table>
</form>
