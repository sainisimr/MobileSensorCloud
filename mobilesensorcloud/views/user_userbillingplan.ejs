<div  style="color:white;margin-left:128px;"><H2>Billing Plan Information<H2></div>
</br>
<div style="color:white;margin-left:58px;">
<form style="margin: 4px 4px 4px 4px;" method="get" action="/user_payment" id="form1" > 
<input type="hidden" name="paymentid" value="" id="paymentid"/>
<table class="container" style="border:1px solid white;">
<tr style="background-color:#2c3e50; border: 1px solid white;">
<th style="border: 1px solid white; padding:4px 10px 4px 10px;">Billing Id</th>
<th style="border: 1px solid white; padding:4px 10px 4px 10px;">Billing Plan</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;"> Cost </th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Description</th>
<th style="border: 1px solid white;padding:4px 10px 4px 10px;">Status</th>

</tr>
 <% for(var i in data) 
	            {                     
            %>
<tr style="background-color:#2c3e50; border: 1px solid white;">
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].billingid_primarykey %> <br/>  
				
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">

					 <%= data[i].planname %> <br/>  
				
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					<%= data[i].cost %>$ <br/>  
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">
					<%= data[i].description %> <br/>  
 </td>
<td style="border: 1px solid white;padding:4px 10px 4px 10px;">
		<% if(data[i].billinginformations!=null){%>
		<%= data[i].billinginformations %>
		<%}else{%>
		<button type="button" value="payment" onclick="javascript:enroll('/user_enrollplan','<%= data[i].billingid_primarykey %>');">Enroll</button> 
		<%}%>
		
 </td> 
</tr>
<% 
	            }  
	       %>
</table>
</form>
